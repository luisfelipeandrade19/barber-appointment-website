{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "proxy": "http://127.0.0.1:5000",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "playwright test --config=playwright/playwright.config.ts",
    "test:ui": "playwright test --ui --config=playwright/playwright.config.ts",
    "test:headed": "playwright test --headed --config=playwright/playwright.config.ts",
    "test:prod": "npm run build && playwright test --config=playwright/playwright.config.prod.ts",
    "test:prod:ui": "npm run build && playwright test --ui --config=playwright/playwright.config.prod.ts",
    "test:prod:headed": "npm run build && playwright test --headed --config=playwright/playwright.config.prod.ts",
    "test:report": "playwright show-report"
  },
  "dependencies": {
    "@greatsumini/react-facebook-login": "^3.4.0",
    "@react-navigation/bottom-tabs": "^7.8.11",
    "@react-oauth/google": "^0.12.2",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router-dom": "^7.10.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@playwright/test": "^1.48.0",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "playwright": "^1.48.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^7.2.4"
  }
}